# Server Configuration
PORT=3002
NODE_ENV=development

# AI API Configuration
THIRD_API_URL=https://api.openai.com/v1
THIRD_API_KEY=your_api_key_here

# AI Models Configuration (JSON format)
# Example: [{"modelName":"GPT-4","modelKey":"gpt-4","useImage":true,"provider":"openai","functionCall":true}]
AI_MODELS_CONFIG=[]
AI_DEFAULT_MODEL=gemini-3-pro-preview

# Screenshot Service
SCREENSHOTONE_API_KEY=your_screenshotone_api_key

# Netlify Deployment
NETLIFY_TOKEN=your_netlify_token
NETLIFY_DEPLOY_URL=https://api.netlify.com/api/v1/sites

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# AI Generation Token Limits
# Maximum output tokens for AI generation (default: 8192)
AI_MAX_OUTPUT_TOKENS=8192
# Maximum input tokens for context (default: 128000)
AI_MAX_INPUT_TOKENS=128000
# Token limit for standard prompts (default: 128000)
AI_STANDARD_TOKEN_LIMIT=128000
